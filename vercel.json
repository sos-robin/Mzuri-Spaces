{
    "builds": [
      {
        "src": "mzuri_spaces/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/img/(.*)",
        "dest": "/static/img/$1"
      },
      {
        "src": "/(.*)",
        "dest": "mzuri_spaces/wsgi.py"
      }
    ]
  }
  
